{
  "version": 2,
  "builds": [{
    "src": "index.js",
    "use": "@vercel/node",
    "config": {
      "maxLambdaSize": "50mb",
      "includeFiles": [
        "app.js",
        "routes/**",
        "models/**",
        "node_modules/**"
      ]
    }
  }],
  "routes": [{
    "src": "/api/(.*)",
    "dest": "index.js",
    "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
  }]
}